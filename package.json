		"pa11y-ci": "^3.0.1",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.58.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
